{
  "name": "gaelreyrol.dev",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style",
    "test": "jest",
    "gh-pages": "push-dir --dir=dist --branch=gh-pages --cleanup",
    "resume:json": "dhall-to-json --file dhall/resume.dhall --output static/resume.json",
    "resume:html": "cat static/resume.json | jsonresume-theme-even > static/resume.html",
    "resume": "yarn resume:json && yarn resume:html"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "@nuxt/content": "1.15.1",
    "@nuxtjs/pwa": "3.3.5",
    "@nuxtjs/sitemap": "2.4.0",
    "@tailwindcss/typography": "0.5.9",
    "dayjs": "1.11.7",
    "jsonresume-theme-even": "0.17.1",
    "nuxt": "2.15.8"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.19.1",
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@nuxtjs/eslint-config": "12.0.0",
    "@nuxtjs/eslint-module": "3.1.0",
    "@nuxtjs/stylelint-module": "4.1.0",
    "@nuxtjs/tailwindcss": "6.2.0",
    "@vue/test-utils": "2.2.7",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "29.3.1",
    "eslint": "8.32.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-nuxt": "4.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "jest": "29.3.1",
    "lint-staged": "13.1.0",
    "postcss": "8.4.21",
    "postcss-html": "1.5.0",
    "prettier": "2.8.3",
    "push-dir": "0.4.1",
    "stylelint": "14.16.1",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-recommended-vue": "1.4.0",
    "stylelint-config-standard": "29.0.0",
    "vue-jest": "3.0.7"
  }
}
