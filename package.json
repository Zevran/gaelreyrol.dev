{
  "name": "gaelreyrol.dev",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style",
    "test": "jest",
    "gh-pages": "push-dir --dir=dist --branch=gh-pages --cleanup",
    "resume:json": "dhall-to-json --file dhall/resume.dhall --output static/resume.json",
    "resume:html": "cat static/resume.json | jsonresume-theme-even > static/resume.html",
    "resume": "yarn resume:json && yarn resume:html"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "@nuxt/content": "^1.15.1",
    "@nuxtjs/pwa": "^3.3.0",
    "@nuxtjs/sitemap": "^2.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "dayjs": "^1.10.0",
    "jsonresume-theme-even": "^0.17.0",
    "nuxt": "^2.15.8"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@nuxtjs/eslint-config": "^10.0.0",
    "@nuxtjs/eslint-module": "^3.0.1",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/tailwindcss": "^5.1.2",
    "@vue/test-utils": "^2.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^28.1.1",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "lint-staged": "^13.0.2",
    "postcss": "^8.4.4",
    "postcss-html": "^1.4.1",
    "prettier": "^2.5.1",
    "push-dir": "^0.4.1",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.1.0",
    "stylelint-config-standard": "^26.0.0",
    "vue-jest": "^3.0.4"
  }
}
